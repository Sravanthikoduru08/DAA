def process_list(nums):
    if not nums:  # Check for an empty list
        return "The list is empty"
    
    sorted_nums = sorted(nums)
    return sorted_nums[-1]

# Test cases
test_case1 = []
test_case2 = [5]
test_case3 = [3, 3, 3, 3, 3]

print(process_list(test_case1))  # Expected Output: "The list is empty"
print(process_list(test_case2))  # Expected Output: 5
print(process_list(test_case3))  # Expected Output: 3
